{
  "topic": "goalsetting",
  "variables": {
    "acceptgoal": {
      "agreed{{currentGoal}}Goal": {
        "append": false,
        "value": "$p",
        "clear_on_new": true
      }
    },
    "rejectgoaltoohigh": {
      "rejectedTooHigh": {
        "append": true,
        "value": "$p",
        "clear_on_new": true
      }
    },
    "rejectgoaltoolow": {
      "rejectedTooLow": {
        "append": true,
        "value": "$p",
        "clear_on_new": true
      }
    },
    "end": {
      "{{currentGoal}}Set": {
        "append": false,
        "value": true,
        "clear_on_new": false
      },
      "currentGoal": {
        "append": false,
        "value": "[{{currentGoal}}=longTerm?shortTerm:longTerm]"
      }
    }
  }
}
