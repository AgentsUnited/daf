[
  {
    "protocol": "goalsetting",
    "variables": {
      "Confirm": {
        "agreedgoal": {
          "value": "$p",
          "clear_on_new": true
        }
      },
      "Reject": {
        "rejectedgoal": {
          "value": "$p",
          "append": true,
          "clear_on_new": true
        }
      }
    }
  }
]
